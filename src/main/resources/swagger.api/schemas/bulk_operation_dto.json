{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Bulk Operation",
  "BulkOperationDto": {
    "type": "object",
    "properties": {
      "id": {
        "description": "Bulk Operation UUID",
        "type": "string",
        "format": "uuid"
      },
      "userId": {
        "description": "User UUID",
        "type": "string",
        "format": "uuid"
      },
      "operationType": {
        "description": "Bulk Operation type",
        "$ref": "operation_type.json#/OperationType"
      },
      "entityType": {
        "description": "Entity type",
        "$ref": "entity_type.json#/EntityType"
      },
      "entityCustomIdentifierType": {
        "description": "Entity custom identifier type",
        "$ref": "identifier_type.json#/IdentifierType"
      },
      "status": {
        "description": "Bulk Operation status",
        "$ref": "operation_status_type.json#/OperationStatusType"
      },
      "approach": {
        "description": "Bulk Operation approach type",
        "$ref": "approach_type.json#/ApproachType"
      },
      "dataExportJobId": {
        "description": "Data export Job UUID",
        "type": "string",
        "format": "uuid"
      },
      "linkToOriginFile": {
        "description": "Path to the origin file",
        "type": "string"
      },
      "linkToMatchingRecordsFile": {
        "description": "Path to the origin file",
        "type": "string"
      },
      "linkToModifiedFile": {
        "description": "Path to the modified file",
        "type": "string"
      },
      "linkToResultFile": {
        "description": "Path to the result file",
        "type": "string"
      },
      "linkToTriggeringFile": {
        "description": "Path to the bulk operation triggering file",
        "type": "string"
      },
      "linkToMatchingErrorsFile": {
        "description": "Path to the matching errors file",
        "type": "string"
      },
      "linkToCommittingErrorsFile": {
        "description": "Path to the committing errors file",
        "type": "string"
      },
      "linkToThePreviewFile": {
        "description": "Path to the committing errors file",
        "type": "string"
      },
      "totalNumOfRecords": {
        "description": "Total number of records",
        "type": "integer",
        "format": "int64",
        "default": 0
      },
      "processedNumOfRecords": {
        "description": "Number of processed records",
        "type": "integer",
        "format": "int64",
        "default": 0
      },
      "executionChunkSize": {
        "description": "Size of chunk",
        "type": "integer",
        "format": "int64",
        "default": 0
      },
      "startTime": {
        "description": "Bulk Operation start time",
        "type": "string",
        "format": "date-time"
      },
      "endTime": {
        "description": "Bulk Operation end time",
        "type": "string",
        "format": "date-time"
      },
      "errorMessage": {
        "description": "Error message",
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "type"
    ]
  }
}
